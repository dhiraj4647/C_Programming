#include <stdio.h>

int main() {
    // Write C code here
    int i=1,j=1,k,num=0;
    printf("Enter the number of rows::");
    scanf("%d",&num);
    for(i=0;i<num;i++){
        for(k=0;k<i;k++){
            printf(" ");
        }
        for(j=1;j<=num-i;j++){
            printf("%d",j);
        }
        printf("\n");
    }
    return 0;
}

output:
Enter the number of rows::5
12345
 1234
  123
   12
    1
