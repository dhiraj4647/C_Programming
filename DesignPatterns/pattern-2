#include <stdio.h>

int main() {
    // Write C code here
    int i=1,j=1,num=0;
    printf("Enter the number of rows::");
    scanf("%d",&num);
    for(i=0;i<num;i++){
        for(j=num-i;j>=1;j--){
            printf("%d ",j);
        }
        printf("\n");
    }
    return 0;
}

output:-
Enter the number of rows::5
5 4 3 2 1 
4 3 2 1 
3 2 1 
2 1 
1 
